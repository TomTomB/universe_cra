{
  "files.associations": {
    ".babelrc": "jsonc",
    ".eslintrc": "jsonc",
    ".prettierrc": "jsonc",
    ".eslintignore": "ignore"
  },

  "javascript.validate.enable": false,
  "javascript.format.enable": false,
  "typescript.format.enable": false,

  "search.exclude": {
    ".git": true,
    ".eslintcache": true,
    "src/dist": true,
    "src/main.prod.js": true,
    "src/main.prod.js.map": true,
    "bower_components": true,
    "dll": true,
    "release": true,
    "node_modules": true,
    "npm-debug.log.*": true,
    "test/**/__snapshots__": true,
    "yarn.lock": true,
    "*.{css,sass,scss}.d.ts": true
  },
  "cSpell.words": [
    "Ahri",
    "Bloodmoon",
    "Coord",
    "Flyout",
    "Formik",
    "Outro",
    "Pulsfire",
    "Reduxie",
    "Styleguide",
    "borderless",
    "bornholdt",
    "caitlyn",
    "caitlynpulsefire",
    "clearall",
    "cond",
    "darkstar",
    "dianabloodmoon",
    "eslintignore",
    "fullscreenable",
    "galio",
    "gamedata",
    "glsl",
    "immer",
    "irelia",
    "kaisa",
    "opentype",
    "overlayscrollbars",
    "prettierrc",
    "reduxjs",
    "semibold",
    "shentox",
    "spritesheet",
    "starguardian",
    "starguardians",
    "truetype",
    "uikit",
    "ungrouped",
    "unranked",
    "unselectable",
    "webgl",
    "webm",
    "webp",
    "xayahrakan",
    "xinzhaodragonslayer",
    "xstate",
    "zustand"
  ]
}
